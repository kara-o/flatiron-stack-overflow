
<h1>Flatiron's Stack Overflow</h1>
<h2>Welcome!</h2>
<br>


<%= form_with model: @user do |f| %>
  <%= f.label :display_name, "Username" %>
  <%= f.text_field :display_name %><br>

  <%= f.label :email_address, "Email" %>
  <%= f.text_field :email_address %><br>

  <%= f.submit "Login" %>
<% end %>
